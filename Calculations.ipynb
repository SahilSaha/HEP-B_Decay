   "source": [
    "import numpy as np\n",
    "\n",
    "mB = 5.27934\n",
    "mD = 2.00685\n",
    "mPi = 0.13957\n",
    "\n",
    "ED = (mB**2+mD**2-mPi**2)/(2*mB)\n",
    "EPi = (mB**2+-mD**2+mPi**2)/(2*mB)\n",
    "pD = np.sqrt(ED**2-mD**2)\n",
    "pPi = np.sqrt(EPi**2-mPi**2)\n",
    "\n",
    "print(ED,EPi,pD,pPi)"
   ]
